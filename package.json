{
  "name": "dreams",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "bun": ">=1.1.0"
  },
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "dev:all": "./scripts/start-all.sh",
    "stop": "./scripts/stop-all.sh",
    "start": "bun run src/index.ts",
    "start:api": "bun run src/api/server.ts",
    "worker": "bun run scripts/worker.ts",
    "build": "bun build api/index.ts --outfile api/index.js --target node --format esm --minify --external @vercel/node",
    "agent": "bun run src/agent.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "check": "bun run scripts/test-setup.ts",
    "typecheck": "bunx tsc --noEmit",
    "test": "bun test",
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "test:e2e": "bun test tests/e2e"
  },
  "dependencies": {
    "@lucid-dreams/agent-kit": "latest",
    "@pacoyang/agent-twitter-client": "^0.0.18",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.5",
    "openai": "^6.7.0",
    "viem": "^2.38.6",
    "x402-fetch": "^0.7.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.7.2",
    "@vercel/node": "^5.5.3",
    "bun-types": "^1.3.0",
    "drizzle-kit": "^0.31.6",
    "playwright": "^1.56.1",
    "typescript": "^5.9.2"
  }
}
