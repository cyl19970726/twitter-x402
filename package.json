{
  "name": "dreams",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "bun": ">=1.1.0"
  },
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target node --minify",
    "build:vercel": "mkdir -p api && bun build src/index.ts --outfile api/index.js --target node --format esm",
    "agent": "bun run src/agent.ts",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@lucid-dreams/agent-kit": "latest",
    "@pacoyang/agent-twitter-client": "^0.0.18",
    "openai": "^6.7.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "bun-types": "^1.3.0",
    "playwright": "^1.56.1",
    "typescript": "^5.9.2"
  }
}
